{
  "manifest_version": 2,
  "name": "Jupyter Keeper",
  "description": "An extention which prevents Chrome from reloading Jupyter Notebook",
  "version": "1.0.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Jupyter Keeper",
    "default_icon":"icon.png"
  },
  "icons": {
    "16":"icon.png",
    "48":"icon.png",
    "128":"icon.png"
  },
  "permissions": ["contextMenus", "tabs"],
  "default_locale": "en"
}
